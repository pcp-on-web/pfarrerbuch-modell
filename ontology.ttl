## This is the model interlinking Pfarrerbücher

@prefix : <http://pfarrerbuch.de/ontology#> .
@prefix pbo: <http://pfarrerbuch.de/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/terms/>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://pfarrerbuch.de/ontology#> rdf:type owl:Ontology ;
	<http://creativecommons.org/ns#attributionName> "Participants of Workshop" ;
	dc:title "Vokabular Pfarrerbücher"@de ;
	dc:license "http://creativecommons.org/licenses/by/4.0/" ;
	dc:modified "2023-03-19".
	
:Pastor rdf:type owl:Class ;
  rdfs:label "Pfarrer"@de ;
	rdfs:comment "Stellt ein Person dar, die in einem Pfarrebuch verzeichneit ist."@de .				
		
:Place rdf:type owl:Class ;
  rdfs:label "Ort"@de ;
	rdfs:comment "Ort in einem Pfarrerbuch"@de .
					
:Church rdf:type owl:Class ;
  rdfs:label "Kirche"@de ;
	rdfs:comment "Kirche in einem Pfarrerbuch"@de .
	
:Postion rdf:type owl:Class ;
  rdfs:label "Pfarstelle"@de ;
	rdfs:comment "Eine Pfarrstelle innerhalb einer Kirchgemeinde"@de .
	
:StageOfLife rdf:type owl:Class ;
  rdfs:label "Lebensabschnitt"@de ;
	rdfs:comment "Lebensabschnitt eines Pfarrers"@de .


:hasStageOfLife rdf:type owl:ObjectProperty ;
                rdfs:domain :Pfarrer ;
                rdfs:range :StageOfLife ;
                rdfs:label "hat Lebensabschnitt"@de ;
                rdfs:comment "Lebensabschnitt zu einem Pfarrer"@de .


:forename rdf:type owl:DatatypeProperty ;
          rdfs:domain :Pfarrer ;
          rdfs:range rdfs:Literal ;
          rdfs:label "Vorname"@de ;
          rdfs:comment "Vorname(n) einer Person, getrennt mit Leerzeichen"@de .
          
:surname rdf:type owl:DatatypeProperty ;
         rdfs:domain :Pfarrer ;
         rdfs:range rdfs:Literal ;
         rdfs:label "Nachname"@de ;
         rdfs:comment "Nachname einer Person"@de .
          
          





